{
  "name": "tether",
  "description": "A plugin for powerful agent orchestration that prevents over-engineering and scope creep.",
  "version": "1.0.3",
  "author": {
    "name": "crinzo"
  },
  "skills": "./skills/",
  "agents": [
    "./agents/tether-orchestrator.md",
    "./agents/assess.md",
    "./agents/anchor.md",
