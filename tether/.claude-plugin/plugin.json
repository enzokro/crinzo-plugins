{
  "name": "tether",
  "description": "A plugin for powerful agent orchestration that prevents over-engineering and scope creep.",
  "version": "1.0.6",
  "author": {
    "name": "crinzo"
  },
  "skills": "./skills/",
  "agents": [
    "./agents/tether-orchestrator.md",
    "./agents/assess.md",
    "./agents/anchor.md",
    "./agents/code-builder.md",
    "./agents/reflect.md"
  ],
  "commands": "./commands/",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "${CLAUDE_PLUGIN_ROOT}/scripts/register.sh TETHER"
          }
        ]
      }
    ]
  },
  "outputStyles": "./output-styles/"
}