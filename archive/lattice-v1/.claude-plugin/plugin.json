{
  "name": "lattice",
  "description": "Semantic memory for tether workspaces. Hybrid retrieval with embeddings.",
  "version": "1.0.5",
  "author": {
    "name": "crinzo"
  },
  "skills": "./skills/",
  "agents": [
    "./agents/miner.md",
    "./agents/surface.md"
  ],
  "commands": "./commands/",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "${CLAUDE_PLUGIN_ROOT}/scripts/setup.sh"
          },
          {
            "type": "command",
            "command": "${CLAUDE_PLUGIN_ROOT}/scripts/register.sh LATTICE"
          }
        ]
      }
    ]
  },
  "outputStyles": "./output-styles/"
}